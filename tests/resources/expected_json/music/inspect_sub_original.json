    "title": "{track_title}",
    "track": "{track_number}",
    "tracktotal": "{track_total}",
    "year": "{track_year}"
  },
  "output_options": {
    "file_name": "{track_full_path}",
    "maintain_download_archive": true,
    "output_directory": "{music_directory}",
    "thumbnail_name": "{album_cover_path}"
  },
  "overrides": {
    "album_cover_path": "{artist_dir}/{album_dir}/folder.{thumbnail_ext}",
